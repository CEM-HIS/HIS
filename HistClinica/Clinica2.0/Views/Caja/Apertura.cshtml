
@model Clinica2._0.DTO.CitaDTO
<div class="modal-dialog modal-md" style="max-width: 700px;" role="document">
	<div class="modal-content">
		<div class="modal-header">
			<h2 class="modal-title text-center" id="exampleModalLabel1">Apertura de Caja Ambulatoria</h2>
			<button type="button" class="close">
				<span aria-hidden="true">&times;</span>
			</button>
		</div>
		<div class="container-fluid">
			<div class="col-sm-12">
				<div class="row">
					<div class="col-sm-12">
						<div class="input-style">
							<label class="form-label">Fecha:</label>
							<input type="text" name="name" class="form-control" />
						</div>
					</div>
					<div class="col-sm-12">
						<div class="input-style">
							<label class="form-label">Turno:</label>
							<input type="text" name="name" class="form-control" />
						</div>
					</div>
					<div class="col-sm-12">
						<div class="input-style">
							<label class="form-label">Impuesto base:</label>
							<input type="text" name="name" class="form-control" />
						</div>
					</div>
					<div class="col-sm-12">
						<div class="input-style">
							<label class="form-label">Observacion:</label>
							<input type="text" name="name" class="form-control" />
						</div>
					</div>
					<div class="col-sm-12 text-right pb-2">
						<button onclick="" class="btn btn-primary">Apertura</button>
						<a style="color:white;" asp-action="RegistroCita" asp-controller="Cita" class="btn btn-primary">Cerrar</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>